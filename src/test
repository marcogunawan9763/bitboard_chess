    //can D
    if(not_8_rank){
        //can RR
        if ((bitboard >> 6) & not_hg_file){
            attacks |= (bitboard >> 6);
        }
        
        //can LL
        if ((bitboard >> 10) & not_ab_file){
            attacks |= (bitboard >> 10);
        }

    //can DD
    if (not_78_rank){
        //can R
        if ((bitboard >> 15) & not_h_file){
            attacks |= (bitboard >> 15);
        }
        
        //can L
        if ((bitboard >> 10) & not_a_file){
            attacks |= (bitboard >> 10);
        }

    }